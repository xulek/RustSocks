      "Bash(source \"$HOME/.cargo/env\")",
      "Bash(cargo --version:*)",
      "Bash(mkdir:*)",
      "Bash(cargo check:*)",
      "Bash(cargo test:*)",
      "Bash(cargo build:*)",
      "Bash(./target/release/rustsocks:*)",
      "Bash(netstat:*)",
      "Bash(ss:*)",
      "Bash(cargo clippy:*)",
      "Bash(export PATH=\"$HOME/.cargo/bin:$PATH\")",
      "Bash(find:*)",
      "Bash($HOME/.cargo/bin/cargo check)",
      "Bash($HOME/.cargo/bin/cargo test acl)",
